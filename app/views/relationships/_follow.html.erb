<% users.each do |user| %>
  <div class="follow--detail">
    <%= link_to user_path(user), class: "follow--user" do %>
      <%= attachment_image_tag(user, :user_image, fallback: "no_image.jpg") %>
      <%= user.name %>
    <% end %>
    <%= render "relationships/relationship", user: user %>
  </div>
<% end %>


