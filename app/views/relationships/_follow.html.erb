<% if users.exists? %>
  <% users.each do |user| %>
    <%= attachment_image_tag(user, :user_image, :fill, 50, 50, fallback: "no_image.jpg") %></td>
    <%= user.name %>
    フォロー数: <%= user.followings.count %>
    フォロワー数: <%= user.followers.count %>
    <%= link_to 'Show', user_path(user), class: "user_#{user.id}" %>
  <% end %>
<% else %>
  <p>ユーザーはいません</p>
<% end %>